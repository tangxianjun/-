/* pages/collect/collect.wxss */
/* pages/my_sell/sell.wxss */
Page,view,text{
   font-family: PingFang SC;
}
Page{
  background-color: #f9f9f9;
}
.me_kong{
  height: 168rpx;
  padding-top: 38rpx;
  width: 100%;
  border-radius: 0 0 0 140rpx;
  background: rgb(255, 255, 255);
  box-shadow: 2px 2px 10px #eee;
}
.my{
  position: relative;
  height: inherit;
  width: 100%;
  background: #FFFFFF;
  border-radius: 0 0 0 140px;
}
.status_text{
  height: 50rpx;
  line-height: 50rpx;
  width: 380rpx;
  float: left;
  font-size: 36rpx;
}
.text_kong{
  height: 50rpx;
  /* width: 200rpx; */
  max-width: 140rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 36rpx;
  margin-left: 68rpx;
  /* margin-top: 38rpx; */
  line-height: 50rpx;
  float: left;
  overflow: hidden;
}
.text2{
  width: 100%;
  height: 40rpx;
  /* width: 200rpx; */
  float: left;
  line-height: 40rpx;
  margin-left: 68rpx;
  font-size: 28rpx;
  color: #BBBBBB;
  letter-spacing: 0.06px;
  text-align: left;
  margin-top: 5.9rpx;
}
.img{
  height: 100rpx;
  width: 100rpx;
  float: right;
  margin-right: 58rpx;
  /* margin-bottom: 106rpx; */
  border-radius: 100%;
  position: absolute;
  right: 0px;
}
.m_img{
  height: 150rpx;
  width: 150rpx;
  position: absolute;
  top:0px;
  bottom:0px;
  left:0px;
  margin: auto;
  margin-left:44rpx;
  border-radius: 100%;
}
.m_img>image{
  width: 100%;
  height: 100%;
  border-radius:50%;
}
.imgs{
  height: 100rpx;
  width: 100rpx;
}
.backnone{
  height: 730rpx;
  width: 100%;
  margin-top: 108.1rpx;
  text-align: center;
}
.none_img{
  margin-left: 192.5rpx;
  height: 400rpx;
  width: 372rpx;
  display: block;
}
.none_text{
  margin-top: 24.6rpx;
  height: 27rpx;
  font-size: 19.2rpx;
  color: #BBBBBB;
  letter-spacing: 0.04px;
}
.button{
  height: 97rpx;
  width: 425rpx;
  margin-top: 160.8rpx;
  background: #FFE896;
  border-radius: 47px;
  font-size: 34rpx;
  line-height: 97rpx;
  color: #000000;
}
.collect{
  transition: 0.3s all;
  height: 338rpx;
  width: 686rpx;
  margin-top: 20rpx;
  margin-left: 33rpx;
  border-radius: 25rpx;
  background-color: #fff;
  box-shadow: 2px 5px 10px #eee;
}
.h_back{
  position: relative;
  border-radius: 15px 15px 0 0;
  height: 36rpx;
  padding-top: 15rpx;
  background: #FFE896;
}
.right{
  position: relative;
  height: 45rpx;
  width: 100rpx;
  float: right;
  /* display: flex; */
}
.optionlist{
  display: none;
  z-index: 999;
  top:20px;
  position: absolute;
  text-align: center;
  right: 5px;
  width: 140rpx;
  font-size: 24rpx;
  color:#fff;
}
.optionlist::before{
  content: '▲';
  color:rgba(170, 170, 170,0.8);
  position:relative;
  top: 5px;
  left: 20px;
}
.optionlist>view{
  border-radius: 5px;
  background-color:rgba(170, 170, 170,0.8);
  padding: 5px 8px;
}
.r{
  border-radius: 100%;
  margin: 0 7rpx;
  height: 15rpx;
  float: left;
  width: 15rpx;
  display: block;
  background-color:  #FFFFFF;
}
.c_content{
  background: #FFFFFF;
  margin-top: 36rpx;
  height: 212rpx;
  width: 639rpx;
  margin-left: 26rpx;

}
.c_img{
  position: absolute;
  width: 212rpx;
  float: left;
}
.cimg{
  width: 212rpx;
  height: 212rpx;
}
.c_right{
  height: 212rpx;
  width: 406rpx;
  margin-left: 233rpx;
  position: relative;
  float: left;
}
.c_top{
  font-size: 28rpx;
  height: 80rpx;
  width: 406;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp:2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.c_z{
  font-size: 22rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp:2;
  margin-top: 7rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  word-break: break-all;
  height: 60rpx;
  width: 395rpx;
  color: #BBBBBB;
}
.c_time{
  display: inline;
  margin-top: 45rpx;
  float: left;
  font-size: 24rpx;
}
.c_time>image{
  margin-right: 0.5em;
  height: 20rpx;
  width: 20rpx;
}
.c_price{
  float: right;
  margin-top: 30rpx;
  vertical-align: middle;
  display: inline;
}
.price-now{
  font-size: 40rpx;
  font-weight: bold;
  color:#FA6400;
}
.dollar{
  font-weight: bold;
  font-size: 28rpx;
  color:#FA6400;
}
.price-prv{
  font-size: 24rpx;
  text-decoration: line-through;
  color: #BBBBBB;
}
/* 我的消息 */
.news{
  padding-top:10rpx;
  padding-bottom:10rpx;
  height: 208rpx;
  width: 704rpx;
  background: #FFFFFF;
  box-shadow: 0 2px 30px 0 #EDEDED;
  border-radius: 30px;
  margin-left: 25rpx;
  margin-top: 20rpx;
  position: relative;
  transition:0.3s cubic-bezier(0.215, 0.610, 0.355, 1);
}
.news>.right{
  width:40rpx;
  position: absolute;
  right: 50px;
  top: 0px;
  bottom: 0px;
  height: 130rpx;
  margin: auto
}
.newsOption{
  position:absolute;
  right: 20px;
  top: 0px;
  bottom: 0px;
  margin: auto;
  height: 100rpx;
}
.newsOption>image{
  padding: 20rpx;
  width: 60rpx;
  height: 60rpx;
  background-color: #FFE896;
  border-radius: 50%;
  margin-right: 20px;
}
.news>.right>.r{
  
  background-color:#aaa;
  margin-top: 10px;
}
.newsStatus{
  background-color:#ff7911;
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  right: 20px;
  top: 20rpx;
  border-radius: 50%;
  z-index: 999;
}