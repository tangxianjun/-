.window{
  transition: 0.6s cubic-bezier(0.215, 0.610, 0.355, 1);
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
}

.window>image{
  width: 552rpx;
  height: 627rpx;
  position: absolute;
  margin: auto;
  top: -10%;
  bottom: 0;
  left: 0;
  right: 0;
}
