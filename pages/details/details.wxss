  position: relative;
} */
.windo{
  transition: 0.6s cubic-bezier(0.215, 0.610, 0.355, 1);
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
}
.w_bo{
  width: 511rpx;
  height: 627rpx;
  position: absolute;
  margin: auto;
  top: -10%;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius:51rpx 51rpx 51rpx 51rpx;
}
.w_bo>image{
  height: 430rpx;
  width: 510rpx;
  display: block;
  border-radius:51rpx 51rpx 0rpx 0rpx;
  box-shadow: 0rpx 20rpx  25rpx #AAAAAA;
  -webkit-box-shadow:0 0px 20px rgba(138, 138, 138, 0.4); 
  
}
.backg{
  background-color: #FFFFFF;
  box-shadow: 0rpx 0rpx  25rpx #AAAAAA;
  -webkit-box-shadow:0 0px 20px rgba(138, 138, 138, 0.4); 
  width: 511rpx;
  height: 190rpx;
  border-radius:0px 0px 51rpx 51rpx;
}
